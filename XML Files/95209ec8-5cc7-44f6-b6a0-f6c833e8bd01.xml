         <component>
            <section ID="LINK_27f5b757-1534-4e69-9983-6d60df85520e">
               <id root="8f03f720-529d-452e-9c39-2288b2d745aa"/>
               <code code="60561-8" codeSystem="2.16.840.1.113883.6.1" displayName="OTHER SAFETY INFORMATION"/>
               <title>
                  <content styleCode="italics">
                     <content styleCode="bold">Other information</content>
                  </content>
