         <component>
            <section ID="Le20a1dcf-6e03-43ad-b3e3-ad67204db866">
               <id root="a7de8381-b6f7-4098-8b4a-a457f45ac6e7"/>
               <code code="51945-4" codeSystem="2.16.840.1.113883.6.1" displayName="PACKAGE LABEL.PRINCIPAL DISPLAY PANEL"/>
