               <code codeSystem="2.16.840.1.113883.6.1" code="44425-7" displayName="STORAGE AND HANDLING SECTION"/>
               <title>Other information</title>
               <text>
                  <list listType="unordered">
                     <item>each tablet contains: sodium 20 mg</item>
                     <item>store at 20-25°C (68-77°F). Avoid high humidity and excessive heat above 40°C (104°F). </item>
                  </list>
               </text>
               <effectiveTime value="20090526"/>
            </section>
