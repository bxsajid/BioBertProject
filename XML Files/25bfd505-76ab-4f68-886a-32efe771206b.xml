         <component>
            <section ID="i4i_storage_id_c9e21af0-a455-4198-a93d-2ad81ae95950">
               <id root="44c3c284-a3df-4e8e-9dad-4e8677451579"/>
               <code code="44425-7" codeSystem="2.16.840.1.113883.6.1" displayName="STORAGE AND HANDLING SECTION"/>
               <title>Other information</title>
               <text>
                  <list listType="unordered">
                     <item>
                        <caption>•</caption>
                        <content styleCode="bold">each 5 mL (1 tsp) contains:</content> sodium 2 mg </item>
                     <item>
                        <caption>•</caption>do not use if printed neckband is broken or missing </item>
