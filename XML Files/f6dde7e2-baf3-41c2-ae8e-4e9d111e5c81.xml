            <section ID="LINK_e99b5527-ea1e-42f3-9fe2-db753a6fab7e">
               <id root="7b3ca2b9-92ad-4df4-8c75-2e90ad223055"/>
               <code code="55106-9" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - ACTIVE INGREDIENT SECTION"/>
               <title>Active ingredient (in each tablet)</title>
               <text>
                  <paragraph>Acetaminophen 250 mg<br/>Aspirin 250 mg (NSAID*) <br/>Caffeine 65 mg</paragraph>
                  <paragraph>* nonsteroidal anti-inflammatory drug</paragraph>
               </text>
               <effectiveTime value="20181228"/>
            </section>
