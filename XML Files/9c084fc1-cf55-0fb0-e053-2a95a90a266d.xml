                     <id root="9c084fc1-cf5e-0fb0-e053-2a95a90a266d"/>
                     <code code="55105-1" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - PURPOSE SECTION"/>
                     <text>
                        <table width="60%">
                           <col width="65%" align="left" valign="top"/>
                           <col width="35%" align="left" valign="top"/>
                           <thead>
                              <tr>
                                 <th>Active ingredients (in each capsule)</th>
                                 <th>Purposes</th>
                              </tr>
                           </thead>
                           <tbody>
                              <tr>
                                 <td>Glucosamine Sulfate 250 mg</td>
                                 <td>Dietary Supplement</td>
                              </tr>
                              <tr>
                                 <td>Chondroitin Sulfate 200 mg</td>
                                 <td>Dietary Supplement</td>
                              </tr>
                              <tr>
                                 <td>Ibuprofen 100 mg</td>
                                 <td>Analgesic</td>
                              </tr>
                           </tbody>
