               <title>
                  <content styleCode="bold">
                     <content styleCode="italics">Other information</content>
                  </content>
               </title>
               <text>
                  <list listType="unordered" styleCode="Disk">
                     <item>store between 20° to 25°C (68° to 77°F)</item>
                     <item>
                        <content styleCode="bold">do not use if plastic bottle wrap imprinted “sealed for your protection” is broken or missing</content>
                     </item>
                  </list>
               </text>
               <effectiveTime value="20171115"/>
            </section>
         </component>
