            <section ID="i4i_storage_id_cbd68d5a-dc58-4b08-967a-072b055c84ab">
               <id root="ae147c4a-f46c-467a-ba7f-57778f8ff05f"/>
               <code code="44425-7" codeSystem="2.16.840.1.113883.6.1" displayName="STORAGE AND HANDLING SECTION"/>
               <title>
                  <content styleCode="bold">Other information</content>
