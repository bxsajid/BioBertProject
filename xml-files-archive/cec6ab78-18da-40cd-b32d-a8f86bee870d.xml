            <section ID="i4i_storage_id_d9fbc5ef-cca4-4280-bfeb-b2c23778c6e9">
               <id root="39dba9a2-4eef-4dd9-b16f-505a7ffb92ca"/>
               <code code="44425-7" codeSystem="2.16.840.1.113883.6.1" displayName="STORAGE AND HANDLING SECTION"/>
               <title>Other information</title>
               <text>
                  <list listType="unordered">
                     <item>
                        <caption>•</caption>
                        <content styleCode="bold">each teaspoon contains:</content> sodium 2 mg</item>
                     <item>
                        <caption>•</caption>do not use if printed neckband is broken or missing</item>
                     <item>
                        <caption>•</caption>store at 20-25°C (68-77°F) </item>
                     <item>
                        <caption>•</caption>do not freeze</item>
                     <item>
